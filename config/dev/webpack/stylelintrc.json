{
  "extends": [
    "stylelint-config-wordpress/scss"
  ],
  "rules": {
    "declaration-property-unit-whitelist": {
      "line-height": [ "em" ]
    },
    "function-parentheses-space-inside": "always",
    "max-line-length": 100,
    "selector-pseudo-class-parentheses-space-inside": "always"
  }
}
